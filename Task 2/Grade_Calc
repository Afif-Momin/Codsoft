import java.util.*;
class GradeCalc{
	public static void main(String[] args) {
	
    Scanner scan = new Scanner(System.in);

	System.out.print("Enter total number of subjects:");
	int numSubjects = scan.nextInt();
   	int[] marks = new int[numSubjects];
	int TotalMarks=0;
	double Average;
	String[] subject = new String[numSubjects];

    System.out.println();

    
	for(int initial=0;initial<numSubjects;initial++)
	{
		System.out.print("Enter subject name:");
		subject[initial]=scan.next();
	}

    System.out.println();

	System.out.println("Enter marks of subjects:\n");
	for(int initial=0;initial<numSubjects;initial++)
	{
		System.out.println(subject[initial]+": ");
		marks[initial]=scan.nextInt();
	}

    System.out.println();

	System.out.println("Marks of subjects:\n");
	for(int initial=0;initial<numSubjects;initial++)
	{
		System.out.print(subject[initial]+": ");
		System.out.println(marks[initial]);
	}

    System.out.println();
	for(int initial=0;initial<numSubjects;initial++)
	{
		TotalMarks = TotalMarks+marks[initial];
	}
	System.out.println("Total Marks Obtained:"+TotalMarks);
   
	System.out.println();

	System.out.println("Average Marks:");
	Average = (double) TotalMarks/numSubjects;
	System.out.println(Average);
	
	System.out.println();

	char grade;
        if (Average >= 90) 
		{
            grade = 'A';
        } else if (Average >= 80) 
		{
            grade = 'B';
        } else if (Average >= 70) 
		{
            grade = 'C';
        } else if (Average >= 60) 
		{
            grade = 'D';
        } else 
		{
            grade = 'F';
        }
    System.out.println("Grade: " + grade);
	scan.close();
	}

	
}
